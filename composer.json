{
    "name": "panvagenas/skroutz-xml-feed",
    "type": "wordpress-plugin",
    "version": "150903-dev",
    "description": " A WordPress plugin that generates feed for skroutz.gr ",
    "license": "GPL-3.0+",
    "keywords": [],
    "authors": [
        {
            "name": "Panagiotis Vagenas",
            "email": "pan.vagenas@gmail.com",
            "homepage": "https://gr.linkedin.com/in/panvagenas",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "panvagenas/wp-plugin-core": "*",
        "composer/installers": "~1.0",
        "ircmaxell/random-lib": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "symfony/var-dumper": "2.7.*",
        "devster/ubench": "1.*"
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Skroutz\\": "src/"
        },
        "files": [
            "includes/SimpleXMLExtended.php"
        ]
    },
    "archive": {
        "exclude": [
            "/docs",
            "/tests"
        ]
    }
}
